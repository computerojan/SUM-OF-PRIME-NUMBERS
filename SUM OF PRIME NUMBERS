#include <stdio.h>
int prime(int n){
    if(n<=1){
        return 0;
    }for (int i=2;i*i<=n;i++){
        if(n%i==0){
            return 0;
        }
    } return 1;
}
void sum_primes(int n,int *sum){
    *sum = 0;
    int i;
     for (int i=2;i<=n;i++)
     if (prime(i))
     *sum +=i;
}
int main(){
int sum , n;
int *A= &sum;
printf("ENTER A NUMBER:");
scanf("%d", &n);
sum_primes(n, A); 
    printf("**SUM OPF PRIME NUMBER UPTO %d IS: %d\n", n, *A);
    printf("**ADDRESS OF SUM:%d",&sum);
return 0;
}
